    <entity class="EbicsBank">
        <attributes>
            <id name="hostId"/>
            <element-collection name="supportedEbicsVersions"/>
            <element-collection name="supportedOrderTypes"/>
        </attributes>
    </entity>

    <entity class="EbicsUser">
        <attributes>
            <id name="userId"/>
            <element-collection name="permittedOrderTypes"/>
        </attributes>
    </entity>

    <entity class="EbicsPartner">
        <attributes>
            <id name="partnerId"/>
            <element-collection name="bankAccounts"/>
        </attributes>
    </entity>

    <entity class="FileTransfer">
        <attributes>
            <id name="transferId"/>
        </attributes>
    </entity>

</entity-mappings>